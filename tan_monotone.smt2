(set-logic NRAT)
(declare-fun a () Real)
(declare-fun b () Real)
; Restrict to interval where tan is monotonic: (-Ï€/2, Ï€/2)
(assert (> a (- (/ 3.14159 2))))
(assert (< a (/ 3.14159 2)))
(assert (> b (- (/ 3.14159 2))))
(assert (< b (/ 3.14159 2)))
(assert (<= a b))
(assert (not (<= (tan a) (tan b))))
(check-sat)
